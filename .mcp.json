  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--config",
        ".mcp/playwright-firefox-config.json",
        "--browser",
        "firefox"
      ],
      "env": {
        "HOME": "/home/user/Kagami/.mcp/firefox_home"
      }
    }
  }
}
