{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--config",
        ".mcp/playwright-config.json",
        "--browser",
        "chromium",
        "--proxy-server",
        "$HTTPS_PROXY",
        "--no-sandbox"
      ]
    }
  }
}
